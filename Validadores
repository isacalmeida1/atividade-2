package produto;

public class Validadores {
	

		public static boolean validarPeca(String peca){
		  if(peca.length() == 6){
		   return true;
		  }else{
		   return false;
		  }
		
		 
		}

		public static String preco(String preco) {
			return preco.trim();
		}
		
		  public static String eliminarEspaco(String nome){
				return nome.trim();
		  }

}
